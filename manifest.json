{
  "name": "ao-seo-tool",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "http://ao.com/",
  "homepage_url": "http://ao.com/",
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "ao-seo-tool"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}